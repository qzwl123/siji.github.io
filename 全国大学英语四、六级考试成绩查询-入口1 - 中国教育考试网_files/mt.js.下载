//跨域
if(top!=self)
try
{
  parent.location.host;
}catch(e)
{
  var sn=document.domain,i=/(.com.cn|.net.cn|.org.cn|.edu.cn)$/.test(sn)?8:5;
  document.domain=sn.substring(sn.lastIndexOf(".",sn.length-i)+1);
}
